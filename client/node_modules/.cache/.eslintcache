 Object.keys(ranks[0]).map((col) => {\n  //   if (col !== \"imageURL\")\n  //     columns.push(col);\n  // })\n\n  console.log(props.keyword);\n\n  switch (props.type) {\n    case \"position\":\n      return (\n        <div className={classes}>\n          <Select\n            optionList={countries}\n            withImage=\"true\"\n            default={countries[0]}\n          />\n          <Select optionList={tiers} withImage=\"true\" default={tiers[0]} />\n          <Button contents={positionSort} type=\"position\" />\n        </div>\n      );\n    case \"champion\":\n      return (\n        <div className=\"champion-container\">\n          <div className=\"champion-sort-container\">\n            <Button contents={championSort} type=\"champion\" />\n          </div>\n          <div className=\"champion-list-container\">\n            <div className=\"champion-list\">\n              {champions.map((champion) => {\n                if (champion.content.includes(props.keyword))\n                  return (\n                    <li className=\"champion-item\">\n                      <img\n                        className=\"champion-img\"\n                        src={champion.imageURL}\n                        alt={champion.content}\n                      />\n                      <span>{champion.content}</span>\n                    </li>\n                  );\n              })}\n            </div>\n          </div>\n        </div>\n      );\n    case \"rank\":\n      return (\n        <div className=\"champion-rank\">\n          <Table columns={columns} data={ranks} />\n        </div>\n      );\n    default:\n      return <></>;\n  }\n};\n\nexport default Card;\n","/Users/yujin/WP2022/wp.gg/client/src/redux/rootReducers.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/summoners/actionCreator.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/components/Table.js",["95","96","97","98","99","100","101"],[],"import React, { useState } from \"react\";\nimport \"../static/css/TableStyle.css\";\n\nconst Table = ({ columns, data }) => {\n  const colWidth = [\"70\", \"*\", \"64\", \"94\", \"110\", \"94\", \"135\"];\n  return (\n    <table>\n      <colgroup>\n        {colWidth.map((width) => (\n          <col width={width} />\n        ))}\n      </colgroup>\n      <thead>\n        <tr>\n          {columns.map((col) => {\n            var align = \"\";\n            if (col === \"챔피언\") align = \"left\";\n            return (\n              <th key={col} align={align} scope=\"col\">\n                {col}\n              </th>\n            );\n          })}\n        </tr>\n      </thead>\n      <tbody>\n        {data.map((dataum) => {\n          const { rank, imageURL, name, tier, win, pick, ban, opposite } =\n            dataum;\n          const tierImage = \"https://s-lol-web.op.gg/images/icon/icon-tier-\" + tier + \".svg\";\n          return (\n            <tr key={name}>\n              <td>{rank}</td>\n              <td className=\"rank-champion-td\">\n                <a className=\"rank-champion-link\" href=\"#\">\n                  <img\n                    className=\"rank-champion-img rank-champion-td\"\n                    src={imageURL}\n                  />\n                  <span className=\"rank-champion-td name\">{name}</span>\n                </a>\n              </td>\n              <td><img src={tierImage} /></td>\n              <td>{win}%</td>\n              <td>{pick}%</td>\n              <td>{ban}%</td>\n              {opposite.map((img) => {\n                const { first, second, third } = img;\n                return (\n                  <td>\n                    <img className=\"opposite-champion-img\" src={first} />\n                    <img className=\"opposite-champion-img\" src={second} />\n                    <img className=\"opposite-champion-img\" src={third} />\n                  </td>\n                );\n              })}\n            </tr>\n          );\n        })}\n      </tbody>\n    </table>\n  );\n};\n\