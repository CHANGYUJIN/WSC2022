ssName=\"champion-list-container\">\n            <div className=\"champion-list\">\n              {props.contents.map((champion) => {\n                const { name, image, tags } = champion;\n                if (name.includes(props.keyword))\n                  return (\n                    <li className=\"champion-item\">\n                      <img\n                        className=\"champion-img\"\n                        src={\n                          \"https://opgg-static.akamaized.net/images/lol/champion/\" +\n                          image.full +\n                          \"?image=c_crop,h_103,w_103,x_9,y_9/q_auto,f_webp,w_92&v=1657294091517\"\n                        }\n                        alt={name}\n                      />\n                      <span>{name}</span>\n                    </li>\n                  );\n              })}\n            </div>\n          </div>\n        </div>\n      );\n    case \"rank\":\n      return (\n        <div className=\"champion-rank\">\n          <Table columns={columns} data={ranks} />\n        </div>\n      );\n    default:\n      return <></>;\n  }\n};\n\nexport default Card;\n","/Users/yujin/WP2022/wp.gg/client/src/redux/rootReducers.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/summoners/actionCreator.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/components/Table.js",["115","116","117","118","119","120","121"],[],"import React, { useState } from \"react\";\nimport \"../static/css/TableStyle.css\";\n\nconst Table = ({ columns, data }) => {\n  const colWidth = [\"70\", \"*\", \"64\", \"94\", \"110\", \"94\", \"135\"];\n  return (\n    <table>\n      <colgroup>\n        {colWidth.map((width) => (\n          <col width={width} />\n        ))}\n      </colgroup>\n      <thead>\n        <tr>\n          {columns.map((col) => {\n            var align = \"\";\n            if (col === \"챔피언\") align = \"left\";\n            return (\n              <th key={col} align={align} scope=\"col\">\n                {col}\n              </th>\n            );\n          })}\n        </tr>\n      </thead>\n      <tbody>\n        {data.map((dataum) => {\n          const { rank, imageURL, name, tier, win, pick, ban, opposite } =\n            dataum;\n          const tierImage = \"https://s-lol-web.op.gg/images/icon/icon-tier-\" + tier + \".svg\";\n          return (\n            <tr key={name}>\n              <td>{rank}</td>\n              <td className=\"rank-champion-td\">\n                <a className=\"rank-champion-link\" href=\"#\">\n                  <img\n                    className=\"rank-champion-img rank-champion-td\"\n                    src={imageURL}\n                  />\n                  <span className=\"rank-champion-td name\">{name}</span>\n                </a>\n              </td>\n              <td><img src={tierImage} /></td>\n              <td>{win}%</td>\n              <td>{pick}%</td>\n              <td>{ban}%</td>\n              {opposite.map((img) => {\n                const { first, second, third } = img;\n                return (\n                  <td>\n                    <img className=\"opposite-champion-img\" src={first} />\n                    <img className=\"opposite-champion-img\" src={second} />\n                    <img className=\"opposite-champion-img\" src={third} />\n                  </td>\n                );\n              })}\n            </tr>\n          );\n        })}\n      </tbody>\n    </table>\n  );\n};\n\nexport default Table;\n","/Users/yujin/WP2022/wp.gg/client/src/components/Select.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/components/Button.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/summoners/reducers.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/summoners/actions.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/champions/reducers.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/champions/actions.js",[],[],"/Users/yujin/WP2022/wp.gg/client/src/redux/champions/actionCreator.js",[],[],{"r